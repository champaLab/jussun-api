{
    "name": "jutsun",
    "version": "1.0.0",
    "description": "jutsun",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon --delay 800ms src/index.ts",
        "start": "ts-node src/index.ts",
        "serv": "pm2-runtime start dist/index.js --name JUTSUN-API",
        "serve": "pm2 start ./dist/index.js --name jutsun && pm2 save",
        "git:pull": "git fetch --all && git merge --no-edit origin/master",
        "git:reset": "git reset --hard origin/master",
        "prod": "yarn git:pull && rm -rf .env && cp -rp .env.prod .env && yarn && yarn db && docker-compose up -d --build",
        "build": "tsc",
        "test:watch": "jest --watch",
        "db": "yarn db:pull && yarn db:generate",
        "db:pull": "yarn prisma db pull",
        "db:generate": "yarn prisma generate"
    },
    "prisma": {
        "schema": "./src/prisma/schema.prisma"
    },
    "keywords": [],
    "author": "",
    "overrides": {
        "cache-manager": "5.2.2"
    },
    "dependencies": {
        "@types/request-ip": "^0.0.41",
        "axios": "^1.7.2",
        "cls-hooked": "^4.2.2",
        "cors": "^2.8.5",
        "dayjs": "^1.11.12",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "express-rate-limit": "^7.4.0",
        "express-validator": "^7.1.0",
        "fs-extra": "^11.2.0",
        "jest": "^29.7.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.11.0",
        "node-cron": "^3.0.3",
        "prom-client": "^15.1.3",
        "request-ip": "^3.3.0",
        "rotating-file-stream": "^3.2.3",
        "ts-jest": "^29.2.3",
        "ts-node": "^10.9.2",
        "ua-parser-js": "^2.0.0-beta.3",
        "uuid": "^10.0.0",
        "winston": "^3.13.1",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@prisma/client": "^5.17.0",
        "@types/cls-hooked": "^4.3.6",
        "@types/cors": "^2.8.17",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.19",
        "@types/fs-extra": "^11.0.2",
        "@types/jsonwebtoken": "^9.0.3",
        "@types/multer": "^1.4.11",
        "@types/node": "^22.0.0",
        "@types/node-cron": "^3.0.11",
        "@types/supertest": "^6.0.2",
        "@types/ua-parser-js": "^0.7.39",
        "@types/uuid": "^10.0.0",
        "@types/xlsx": "^0.0.36",
        "nodemon": "^3.1.4",
        "prisma": "^5.17.0",
        "typescript": "^5.5.4",
        "winston-daily-rotate-file": "^5.0.0"
    }
}
