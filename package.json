{
    "name": "jussun-api",
    "version": "1.0.0",
    "description": "jussun-api",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon --delay 100ms src/index.ts",
        "start": "ts-node src/index.ts",
        "serve": "pm2-runtime ./dist/index.js",
        "build": "tsc",
        "test:watch": "jest --watch",
        "update:prisma": "yarn add --dev prisma@latest && yarn add @prisma/client@latest",
        "db": "yarn update:prisma && yarn db:pull && yarn db:generate",
        "db:pull": "yarn prisma db pull",
        "db:generate": "yarn prisma generate"
    },
    "prisma": {
        "schema": "./src/prisma/schema.prisma"
    },
    "keywords": [],
    "author": "",
    "overrides": {
        "cache-manager": "5.2.2"
    },
    "dependencies": {
        "@prisma/client": "^6.6.0",
        "axios": "^1.7.8",
        "bcrypt": "^5.1.1",
        "cls-hooked": "^4.2.2",
        "concurrently": "^9.1.0",
        "cors": "^2.8.5",
        "dayjs": "^1.11.12",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.4.1",
        "express-validator": "^7.2.0",
        "fs-extra": "^11.2.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.3",
        "nodejs": "file:",
        "nodemon": "^3.1.7",
        "prom-client": "^15.1.3",
        "request-ip": "^3.3.0",
        "rotating-file-stream": "^3.2.3",
        "supertest": "^7.0.0",
        "swagger-autogen": "^2.23.7",
        "swagger-ui-express": "^5.0.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.19.2",
        "typescript": "^5.6.3",
        "uuid": "^10.0.0",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cls-hooked": "^4.3.9",
        "@types/cors": "^2.8.17",
        "@types/dotenv": "^8.2.3",
        "@types/express": "^5.0.0",
        "@types/fs-extra": "^11.0.4",
        "@types/http-status-codes": "^1.2.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.10.1",
        "@types/node-cron": "^3.0.11",
        "@types/readable-stream": "^4.0.18",
        "@types/request-ip": "^0.0.41",
        "@types/swagger-ui-express": "^4.1.7",
        "@types/uuid": "^10.0.0",
        "@types/ws": "^8.5.13",
        "prisma": "^6.6.0"
    }
}
