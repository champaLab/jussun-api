CREATE USER 'jussun' @'%' IDENTIFIED BY 'jussun@^9999';
ALTER USER 'jussun' @'%' IDENTIFIED BY 'jussun@^9999';
GRANT ALL PRIVILEGES ON jussun.* TO 'jussun' @'%';
FLUSH PRIVILEGES;
